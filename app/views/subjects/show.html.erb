<h1><%=@subject.id%></h1>
<%= image_tag @subject.image_url, class:"col-md-6"%>


<h2>Annotations</h2>
<row>
  <% @subject.collect_annotations.each_pair do |key,anns|%>
    <div class ="col-md-3">
      <div class="panel panel-primary">
        <div class="panel-heading"><%= key %></div>
        <div class="panel-body panel-overflow">
          <ul class="list-group">
            <%anns.each do |ann|%>
              <li class='list-group-item'> <%= ann[:fields].values.first %> <span class="badge"><%=ann[:user_id][0..5]%></span></li>
            <%end%>
          </ul>
        </div>
      </div>
    </div>
  <%end%>
</row>
